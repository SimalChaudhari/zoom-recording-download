# Zoom Recording Download Automation - Environment Variables

# Server Configuration
PORT=3000
DOWNLOAD_FOLDER=downloads
PAGE_SIZE=100

# Active Tenant Selection
# Set to 'true' to use old tenant, 'false' or unset to use new tenant
USE_OLD_TENANT=false
USE_NEW_TENANT=true

# Old Tenant Configuration (Current)
ZOOM_CLIENT_ID_OLD=your_old_client_id
ZOOM_CLIENT_SECRET_OLD=your_old_client_secret
ZOOM_ACCOUNT_ID_OLD=your_old_account_id
ZOOM_CLOUD_API_OLD=https://api.zoom.us/v2

# New Tenant Configuration (Merged)
ZOOM_CLIENT_ID_NEW=your_new_client_id
ZOOM_CLIENT_SECRET_NEW=your_new_client_secret
ZOOM_ACCOUNT_ID_NEW=your_new_account_id
ZOOM_CLOUD_API_NEW=https://api.zoom.us/v2

# Optional: Specific User IDs to process
USER_IDS=user1@example.com,user2@example.com

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001 